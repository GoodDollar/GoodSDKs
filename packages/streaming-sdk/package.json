{
  "name": "@goodsdks/streaming-sdk",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "latest",
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "latest",
    "viem": "latest",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "viem": "*"
  },
  "dependencies": {
    "@sfpro/sdk": "^0.1.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.2",
    "tsup": "^8.3.5"
  }
}
