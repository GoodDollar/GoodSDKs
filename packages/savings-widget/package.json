{
  "name": "@goodsdks/savings-widget",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "yarn build-savings",
    "dev": "tsc --watch",
    "build-savings": "tsup --config tsup.config.savings.ts",
    "bump-savings": "yarn version patch && yarn build-savings && git add package.json && git commit -m \"version bump\""
  },
  "main": "./dist/index.global.js",
  "module": "./dist/index.js",
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "tsup": "^8.3.5",
    "typescript": "latest"
  },
  "dependencies": {
    "lit": "^3.1.0",
    "viem": "latest"
  },
  "files": [
    "dist",
    "src"
  ]
}
