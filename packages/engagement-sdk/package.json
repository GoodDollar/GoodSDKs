{
  "name": "@goodsdks/engagement-sdk",
  "version": "1.0.3",
  "type": "module",
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsc --watch",
    "bump": "yarn version patch && yarn build && git add package.json && git commit -m \"version bump\""
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/react": "^18",
    "react": "^18",
    "typescript": "latest",
    "viem": "latest",
    "wagmi": "latest"
  },
  "peerDependencies": {
    "react": "*",
    "viem": "*",
    "wagmi": "*"
  },
  "dependencies": {
    "@goodsdks/engagement-contracts": "*",
    "tsup": "^8.3.5"
  }
}
